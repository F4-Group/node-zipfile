{
  "name": "zipfile",
  "version": "0.5.12",
  "main": "./lib/index.js",
  "description": "C++ library for handling zipfiles in node",
  "keywords": [
    "zipfile",
    "uncompress",
    "unzip",
    "zlib"
  ],
  "url": "https://github.com/F4-Group/node-zipfile",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/F4-Group/node-zipfile.git"
  },
  "author": "Dane Springmeyer <dane@dbsgeo.com>",
  "contributors": [],
  "licenses": [
    "BSD"
  ],
  "dependencies": {
    "nan": "~2.18.0",
    "@mapbox/node-pre-gyp": "~1.0.11"
  },
  "devDependencies": {
    "@mapbox/cloudfriend": "^1.9.1",
    "d3-queue": "^3.0.7",
    "mkdirp": "~0.5.1",
    "mocha": "~5.2.0",
    "node-pre-gyp-github": "^1.4.4"
  },
  "bin": {
    "unzip.js": "./bin/unzip.js"
  },
  "scripts": {
    "test": "mocha -R spec",
    "install": "node-pre-gyp install --fallback-to-build",
    "package": "./node_modules/.bin/node-pre-gyp package",
    "publish": "node-pre-gyp-github publish"
  },
  "binary": {
    "module_name": "zipfile",
    "module_path": "./lib/binding/{node_abi}-{platform}-{arch}",
    "host": "https://github.com/F4-Group/node-zipfile/releases/download/",
    "remote_path": "{version}",
    "package_name": "{node_abi}-{platform}-{arch}.tar.gz"
  }
}
